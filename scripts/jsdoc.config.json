{
	"plugins": ["plugins/markdown", "plugins/summarize", "plugins/underscore"],
	"recurseDepth": 10,
	"source": {
		"include": ["src"],
		"includePattern": ".+\\.m?js$",
		"excludePattern": "\\.test\\.m?js$"
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc", "closure"]
	},
	"templates": {
		"referenceTitle": "My SDK Name",
		"cleverLinks": false,
		"monospaceLinks": false,
		"default": {
			"includeDate": false
		}
	},
	"opts": {
		"template": "./node_modules/minami",
		"destination": "./docs",
		"encoding": "utf8",
		"recurse": true
	}
}
